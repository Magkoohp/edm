  <xsd:simpleType name="TwelveTwoAmountType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>A data format for money. The format allows a maximum of 10 positions before the decimal marker (point) and requires 2 position after the decimal marker. The first position should not be a zero.</ccts:Definition>
        <ccts:PrimaryRepresentationTerm>Amount</ccts:PrimaryRepresentationTerm>
        <ccts:DictionaryEntryName>TwelveTwo Amount. Type</ccts:DictionaryEntryName>
        <ccts:PrimitiveType>Decimal</ccts:PrimitiveType>
        <ccts:DataTypeQualifierTerm>TwelveTwo</ccts:DataTypeQualifierTerm>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="udt:AmountType">
      <xsd:pattern value="[0-9]{1,10}\.[0-9]{1,2}"/>
    </xsd:restriction>
  </xsd:simpleType>
