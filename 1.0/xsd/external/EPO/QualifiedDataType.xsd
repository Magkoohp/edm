  <xsd:simpleType name="NameMaxLength120NameType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>A string of characters of maximum length 120</ccts:Definition>
        <ccts:PrimaryRepresentationTerm>Name</ccts:PrimaryRepresentationTerm>
