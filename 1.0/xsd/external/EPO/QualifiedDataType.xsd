  <xsd:simpleType name="SixThreePercentType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>This patters allow 3 digits before the decimal marker and 3 digits after the decimal marker, like in 125.567 %</ccts:Definition>
        <ccts:PrimaryRepresentationTerm>Percent</ccts:PrimaryRepresentationTerm>
        <ccts:DictionaryEntryName>SixThree Percent. Type</ccts:DictionaryEntryName>
        <ccts:PrimitiveType>Decimal</ccts:PrimitiveType>
        <ccts:DataTypeQualifierTerm>SixThree</ccts:DataTypeQualifierTerm>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:restriction base="udt:PercentType">
      <xsd:pattern value="[0-9]{1,3}\.[0-9]{1,3}"/>
    </xsd:restriction>
  </xsd:simpleType>
