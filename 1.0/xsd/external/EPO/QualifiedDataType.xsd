  <xsd:complexType name="SpecificsCodeType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Acronym>DT</ccts:Acronym>
        <ccts:DictionaryEntryName>Specifics Code. Type</ccts:DictionaryEntryName>
        <ccts:Definition>.</ccts:Definition>
        <ccts:PrimaryRepresentationTerm>Code</ccts:PrimaryRepresentationTerm>
        <ccts:DataTypeQualifierTerm>Specifics</ccts:DataTypeQualifierTerm>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:simpleContent>
      <xsd:extension base="clmeCODEXSpecificsCode:SpecificsContentType">
        <xsd:attribute use="optional" fixed="" type="xsd:token" name="listID"/>
        <xsd:attribute use="optional" fixed="eCODEX" type="clm63055:AgencyIdentificationCodeContentType" name="listAgencyID"/>
        <xsd:attribute use="optional" fixed="" type="xsd:token" name="listVersionID"/>
        <xsd:attribute use="optional" fixed="" type="xsd:token" name="listURI"/>
      </xsd:extension>
    </xsd:simpleContent>
  </xsd:complexType>
