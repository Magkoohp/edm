<!-- ===== Type Definition: SumType -->
<!-- =================================================================== -->
  <xsd:complexType name="SumType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>An amount of payment&#13;</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Sum. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>Sum</ccts:ObjectClassTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="qdt:TwelveTwoAmountType" name="Amount" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The total of payment awarded to the claimant, including the principal and, where applicable, interest, contractual penalties and costs;&#13;</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Sum</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Amount</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Amount</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Sum. Amount. Amount</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="udt:IndicatorType" name="AmountUnknownIndicator" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>Indicating that the amount has yet to be determined</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Sum</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Amount Unknown</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Indicator</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Sum. Amount Unknown. Indicator</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="qdt:CurrencyCodeType" name="CurrencyCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>A system of money in general use in a particular country</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Sum</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Currency</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Code</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Sum. Currency. Code</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="qdt:SpecificationOfSumCodeType" name="SpecificationCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>Clear details in order to identify and to support the procedure</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Sum</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Specification</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Code</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Sum. Specification. Code</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:InterestType" name="Interest" minOccurs="0" maxOccurs="unbounded">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>Interest that is to be calculated with respect to the sum.</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>0..unbounded</ccts:Cardinality>
            <ccts:ObjectClassTerm>Sum</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Interest</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Interest</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Sum. Interest. Interest</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="qdt:StringMaxLength200TextType" name="Description" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The list of the  circumstances and the types of evidence that are usually produced in support of pecuniary claims, provided in the application form</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Sum</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Description</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Text</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Sum. Description. Text</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
</xsd:schema>
