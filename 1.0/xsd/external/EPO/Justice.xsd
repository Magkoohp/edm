<!-- ===== Type Definition: CommunicationType -->
<!-- =================================================================== -->
  <xsd:complexType name="CommunicationType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>Specification of possible means of communication with for a specific partner (e.g. telephone, mobile phone, e-mail, fax, e-codex infrastructure).</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Communication. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>Communication</ccts:ObjectClassTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="qdt:CommuncationChannelsCodeType" name="ChannelCode">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The code specifying the channel or manner in which a communication can be made, such as telephone or email..</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Communication</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Channel</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Code</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Communication. Channel. Code</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="qdt:StringMaxLength120TextType" name="CompleteNumber" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>A text string of characters that make up the complete number for this communication..</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Communication</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Complete Number</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Text</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Communication. Complete Number. Text</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="udt:IdentifierType" name="URIID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The unique identifier of the Uniform Resource Identifier (URI) for this communication, such as a web or an email address.</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Communication</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>URI</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Identifier</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Communication. URI. Identifier</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
