<!-- ===== Type Definition: AddressType -->
<!-- =================================================================== -->
  <xsd:complexType name="AddressType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>Structured Address&#13;</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Address. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>Address</ccts:ObjectClassTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="qdt:StringMaxLength20TextType" name="PostalCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>.</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Address</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Postal Code</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Text</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Address. Postal Code. Text</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:AddressLineType" name="AddressLine" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>.</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Address</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Address Line</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Address Line</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Address. Address Line. Address Line</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="qdt:UsageOfAddressCodeType" name="UsageCode">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>Codelist how the specified address is used</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Address</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Usage</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Code</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Address. Usage. Code</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:CityType" name="City" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>.</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Address</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>City</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>City</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Address. City. City</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:CountryType" name="Country">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>A nation or state of the address</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Address</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Country</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Country</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Address. Country. Country</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- =================================================================== -->
<!-- ===== Type Definition: AddressLineType -->
<!-- =================================================================== -->
  <xsd:complexType name="AddressLineType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>Unstructered address information or complete unstructured address&#13;</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Address Line. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>Address Line</ccts:ObjectClassTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="qdt:StringMaxLength120TextType" name="Description">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The unstructured address or a part thereof</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Address Line</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Description</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Text</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Address Line. Description. Text</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="udt:NumericType" name="LineNumberNumeric">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The order of the line in the unstructured address</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Address Line</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Line Number</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Numeric</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Address Line. Line Number. Numeric</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- =================================================================== -->
<!-- ===== Type Definition: AgreementType -->
<!-- =================================================================== -->
  <xsd:complexType name="AgreementType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>A negotiated and typically legally binding arrangement between parties as to a course of action: a trade agreement a verbal agreement to sell &#13;.</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Agreement. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>Agreement</ccts:ObjectClassTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="qdt:KindOfAgreementCodeType" name="KindCode" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>Codelist of the type of agreement to which the claim relates&#13;</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Agreement</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Kind</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Code</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Agreement. Kind. Code</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="qdt:StringMaxLength120TextType" name="Description" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>A description of the agreement&#13;</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Agreement</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Description</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Text</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Agreement. Description. Text</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
<!--ABIE property  suppressed-->
    </xsd:sequence>
  </xsd:complexType>
<!-- =================================================================== -->
<!-- ===== Type Definition: AttachmentType -->
<!-- =================================================================== -->
  <xsd:complexType name="AttachmentType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>A file that is added to the message</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Attachment. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>Attachment</ccts:ObjectClassTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="udt:IdentifierType" name="ContentID">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>.A unique identifier for the attached file that contains the actual data</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Attachment</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Content ID</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Identifier</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Attachment. Content ID. Identifier</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="qdt:IANAMimeMediaTypeCodeType" name="ContentTypeCode">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>An item in a code list describing the format of the file</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Attachment</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Content Type</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Code</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Attachment. Content Type. Code</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="qdt:StringMaxLength120TextType" name="ContentDescription" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>A description of the contents of the attached file</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Attachment</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Content Description</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Text</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Attachment. Content Description. Text</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- =================================================================== -->
<!-- ===== Type Definition: AuthorityType -->
<!-- =================================================================== -->
  <xsd:complexType name="AuthorityType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>A person or organization having political or administrative power and control &#13;</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Authority. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>Authority</ccts:ObjectClassTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="qdt:StringMaxLength200TextType" name="Name">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>Name of the authority</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Authority</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Name</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Text</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Authority. Name. Text</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:AddressType" name="Address">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The address of the authority.</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Authority</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Address</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Address</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Authority. Address. Address</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- =================================================================== -->
<!-- ===== Type Definition: BankAccountType -->
<!-- =================================================================== -->
  <xsd:complexType name="BankAccountType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>.</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Bank Account. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>Bank Account</ccts:ObjectClassTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="udt:IdentifierType" name="NumberID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The number of the bank account assigned by the bank to identify the contract between the bank and one or more parties</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Bank Account</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Number</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Identifier</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Bank Account. Number. Identifier</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="udt:IdentifierType" name="IBANID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>.</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Bank Account</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>IBAN</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Identifier</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Bank Account. IBAN. Identifier</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="udt:IdentifierType" name="BICID" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>An ISO code assigned by SWIFT to identify a financial institution for the purpose of financial transactions</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Bank Account</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>BIC</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Identifier</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Bank Account. BIC. Identifier</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
<!--ABIE property  suppressed-->
      <xsd:element type="qdt:StringMaxLength50TextType" name="AccountHolder" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The name of the account</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Bank Account</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Account Holder</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Text</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Bank Account. Account Holder. Text</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- =================================================================== -->
<!-- ===== Type Definition: CaseType -->
<!-- =================================================================== -->
  <xsd:complexType name="CaseType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>A legal action, especially one to be decided in a court of law, but not limited to be decided in a court of law&#13;</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Case. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>Case</ccts:ObjectClassTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="jus:CaseIdentificationType" name="IdentificationCaseIdentification" minOccurs="0">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>A unique identifier for a case</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>0..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Case</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Identification</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Identification</ccts:AssociatedObjectClassTerm>
            <ccts:AssociatedObjectClassQualifierTerm>Case</ccts:AssociatedObjectClassQualifierTerm>
            <ccts:DictionaryEntryName>Case. Identification. Case. Identification</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:FilingType" name="Filing">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>.The act of filing the document that is the starting point of a  case.</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Case</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Filing</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Filing</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Case. Filing. Filing</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:AuthorityType" name="CourtAuthority">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The court where the case has been filed.</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Case</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Court</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Authority</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Case. Court. Authority</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:JurisdictionType" name="Jurisdiction" maxOccurs="5">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>the ground for jurisdiction of the court as to the case in question</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>1..5</ccts:Cardinality>
            <ccts:ObjectClassTerm>Case</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Jurisdiction</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Jurisdiction</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Case. Jurisdiction. Jurisdiction</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:ClaimType" name="Claim">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>A demand for something based on the assertion of a right </ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Case</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Claim</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Claim</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Case. Claim. Claim</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:DecisionType" name="Decision">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>a conclusion or resolution reached after consideration by an authority</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Case</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Decision</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Decision</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Case. Decision. Decision</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
      <xsd:element type="jus:PartyType" name="InvolvedParty" minOccurs="2" maxOccurs="20">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>A person named as a party to an action or suit. For some purposes, a party who will really litigate the controversy, as distinguished from one defaulting in the action or consenting to judgment. .</ccts:Definition>
            <ccts:Acronym>ASBIE</ccts:Acronym>
            <ccts:Cardinality>2..20</ccts:Cardinality>
            <ccts:ObjectClassTerm>Case</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Involved Party</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:AssociatedObjectClassTerm>Party</ccts:AssociatedObjectClassTerm>
            <ccts:DictionaryEntryName>Case. Involved Party. Party</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- =================================================================== -->
<!-- ===== Type Definition: CaseIdentificationType -->
<!-- =================================================================== -->
  <xsd:complexType name="CaseIdentificationType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>An identifier for uniquely identifying a case  within the context of a registry&#13;</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Case Identification. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>Identification</ccts:ObjectClassTerm>
        <ccts:ObjectClassQualifierTerm>Case</ccts:ObjectClassQualifierTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="udt:IdentifierType" name="NumberID">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>An identifier assigned to a case in some registry</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>Identification</ccts:ObjectClassTerm>
            <ccts:ObjectClassQualifierTerm>Case</ccts:ObjectClassQualifierTerm>
            <ccts:PropertyTerm>Number</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Identifier</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>Case Identification. Number. Identifier</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- =================================================================== -->
<!-- ===== Type Definition: CityType -->
<!-- =================================================================== -->
  <xsd:complexType name="CityType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>Term used generically today to denote any urban form but applied particularly to large urban settlements.&#13;There are, however, no agreed definitions to separate a city from the large metropolis or the smaller town. &#13;</ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>City. Details</ccts:DictionaryEntryName>
        <ccts:ObjectClassTerm>City</ccts:ObjectClassTerm>
        <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
      </xsd:documentation>
    </xsd:annotation>
    <xsd:sequence>
      <xsd:element type="qdt:NameMaxLength50NameType" name="NameName">
        <xsd:annotation>
          <xsd:documentation>
            <ccts:Definition>The name of the city</ccts:Definition>
            <ccts:Acronym>BBIE</ccts:Acronym>
            <ccts:Cardinality>1..1</ccts:Cardinality>
            <ccts:ObjectClassTerm>City</ccts:ObjectClassTerm>
            <ccts:PropertyTerm>Name</ccts:PropertyTerm>
            <ccts:BusinessProcessContextValue>EPO</ccts:BusinessProcessContextValue>
            <ccts:PrimaryRepresentationTerm>Name</ccts:PrimaryRepresentationTerm>
            <ccts:DictionaryEntryName>City. Name. Name</ccts:DictionaryEntryName>
          </xsd:documentation>
        </xsd:annotation>
      </xsd:element>
    </xsd:sequence>
  </xsd:complexType>
<!-- =================================================================== -->
<!-- ===== Type Definition: ClaimType -->
<!-- =================================================================== -->
  <xsd:complexType name="ClaimType">
    <xsd:annotation>
      <xsd:documentation>
        <ccts:Definition>A demand for something based on the assertion of a right </ccts:Definition>
        <ccts:Acronym>ABIE</ccts:Acronym>
        <ccts:DictionaryEntryName>Claim. Details</ccts:DictionaryEntryName>
