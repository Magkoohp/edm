                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="04">
                    <xsd:annotation>
                        <xsd:documentation>
                            <ccts:Name>Agreement as to succession</ccts:Name>
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
                <xsd:enumeration value="05">
                    <xsd:annotation>
                        <xsd:documentation>
                            <ccts:Name>Any other document</ccts:Name>
                        </xsd:documentation>
                    </xsd:annotation>
                </xsd:enumeration>
            </xsd:restriction>
        </xsd:simpleType>
  
  
