	<xs:complexType name="CanMarryType">
				
				    	<xs:annotation>
				
				    	    <xs:documentation>
				
				    		This class contains elements related to the capacity to marry of a person.
				
				    	    </xs:documentation>
				
				    	</xs:annotation>
				
				    	<xs:sequence>
				
				    		<xs:element name="HasCapacityToMarry" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				
				    			<xs:annotation>
				
				    			    <xs:documentation>
				
				    					The person concerned has the capacity to marry under the national law of the Member State where the public document is issued.
				
				    			    </xs:documentation>
				
				    			</xs:annotation>
				
				    		</xs:element>
				
				    		
				
				    		
				
				    		
				
				    		<xs:element name="AppearsToHaveNoMarriage" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				
				    			<xs:annotation>
				
				    			    <xs:documentation>
				
				    					The person concerned appears to have no marriage in his or her name under the national law of the Member State where the public document is issued.
				
				    			    </xs:documentation>
				
				    			</xs:annotation>
				
				    		</xs:element>
				
				    		
				
				    		
				
				    		
				
				    		<xs:element name="NoKnownImpediment" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				
				    			<xs:annotation>
				
				    			    <xs:documentation>
				
				    					There is no known impediment for the person concerned to marry under the national law of the Member State where the public document is issued.
				
				    			    </xs:documentation>
				
				    			</xs:annotation>
				
				    		</xs:element>
				
				    		
				
				    		
				
				    		
				
				    		<xs:element name="NoKnownImpedimentIntendedSpouse" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				
				    			<xs:annotation>
				
				    			    <xs:documentation>
				
				    					There is no known impediment to a marriage between the person concerned and his or her intended spouse under the national law of the Member State where the public document is issued.
				
				    			    </xs:documentation>
				
				    			</xs:annotation>
				
				    		</xs:element>
				
				    		
				
				    		
				
				    		
				
				    		<xs:element name="NoOppositionknown" type="udt:IndicatorType" minOccurs="1" maxOccurs="1" >
				
				    			<xs:annotation>
				
				    			    <xs:documentation>
				
				    					There is no opposition known to a marriage between the person concerned and his or her intended spouse under the national law of the Member State where the public document is issued.
				
				    			    </xs:documentation>
				
				    			</xs:annotation>
				
				    		</xs:element>
				
				    		
				
				    		
				
				    		
				
				    		
				
				    	</xs:sequence>
				
				    	
				
				    </xs:complexType>
				
				    
				
				    
