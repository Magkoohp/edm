	<xs:complexType name="AbsenceOfCriminalRecordPersonType">
		<xs:annotation>
		    <xs:documentation>
			This class contains elements related to the absence of a criminal record for a person.
		    </xs:documentation>
		</xs:annotation>
		<xs:complexContent>
				<xs:restriction base="PersonType">
				<xs:sequence>
					<xs:element name="Surname" type="cvb:PersonFamilyNameType" minOccurs="1" maxOccurs="unbounded">
						<xs:annotation>
						    <xs:documentation>
								This property contains the surname (i.e. last name or family name) of the person.
						    </xs:documentation>
						</xs:annotation>
					</xs:element>
					
					<xs:element name="Forename" type="cvb:PersonGivenNameType" minOccurs="1" maxOccurs="unbounded">
						<xs:annotation>
						    <xs:documentation>
								This property contains the forename (i.e. the first name or given name) of the person. 
						    </xs:documentation>
						</xs:annotation>
					</xs:element>
					
					<xs:element name="DateOfBirth" type="cvb:PersonBirthDateType" minOccurs="1" maxOccurs="1" >
						<xs:annotation>
						    <xs:documentation>
								This property contains the date of birth of the person.
						    </xs:documentation>
						</xs:annotation>
					</xs:element>
					
					<xs:element name="PlaceOfBirth" type="cva:CoreLocationType" minOccurs="1" maxOccurs="1"/>
					
					<xs:element name="CountryOfBirth" type="cva:CoreLocationType" minOccurs="1" maxOccurs="1"/>
					
					<xs:element name="Sex" type="cvb:PersonGenderCodeType" minOccurs="1" maxOccurs="1"/>
					
					<xs:element name="Nationality" type="cva:JurisdictionType" minOccurs="1" maxOccurs="unbounded"/>
					
					
					<xs:element name="PreviousSurname" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
						    <xs:documentation>
								This property contains the previous surname(s) of the person.
						    </xs:documentation>
						</xs:annotation>
					</xs:element>
					
					
					
					<xs:element name="PreviousForename" type="udt:TextType" minOccurs="0" maxOccurs="unbounded">
						<xs:annotation>
						    <xs:documentation>
								This property contains the previous forename(s) of the person.
						    </xs:documentation>
						</xs:annotation>
					</xs:element>
					
					
					
					
				</xs:sequence>
			</xs:restriction>
		</xs:complexContent>
	</xs:complexType>
	
	<xs:complexType name="AddressType">
		<xs:annotation>
		    <xs:documentation>
			This class contains elements related to an address.
					<!-- Dependency Address->Address -->
					Stereotype cvABIE
		    </xs:documentation>
		</xs:annotation>
		<xs:sequence>
			<xs:element name="StreetPOBox" type="cvb:AddressThoroughfareType" minOccurs="1" maxOccurs="1" >
				<xs:annotation>
				    <xs:documentation>
						This property contains contains the Street PO Box details of the address.
				
				    </xs:documentation>
				</xs:annotation>
			</xs:element>
			
			<xs:element name="Place" type="cvb:AddressPostNameType" minOccurs="1" maxOccurs="1" >
				
				    	<xs:annotation>
				
				    	    <xs:documentation>
				
				    			This property contains the location specification of the address.
				
				    	    </xs:documentation>
				
				    	</xs:annotation>
				
				    </xs:element>
				
				    
				
				    <xs:element name="Country" type="cvb:AddressAdminUnitLocationOneType" minOccurs="1" maxOccurs="1" >
				
				    	<xs:annotation>
				
				    	    <xs:documentation>
				
				    			This property contains the name of the country.
				
				    	    </xs:documentation>
				
				    	</xs:annotation>
				
				    </xs:element>
				
				    
				
				    <xs:element name="PostalCode" type="cvb:AddressPostNameType" minOccurs="1" maxOccurs="1" >
				
				    	<xs:annotation>
				
				    	    <xs:documentation>
				
				    			This property contains the postal code of the address
				
				    	    </xs:documentation>
				
				    	</xs:annotation>
				
				    </xs:element>
				
				    
				
				    
		</xs:sequence>
		
	</xs:complexType>
	
	
